<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0 //EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.th.th1.cscenter.CSCenterDAO">
	<select id="cscenterList" resultType="CSCenterVO">
		select * from cscenter 
		order by num asc
	</select>

	<select id="cscenterCategory" parameterType="CSCenterVO" resultType="CSCenterVO">
		select * from cscenter where category=#{category} 
		order by num asc
	</select>


</mapper>	