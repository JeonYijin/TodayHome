<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0 //EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.th.th1.questions.QuestionsDAO">
	<select id="getQuestionsList" resultType="QuestionsVO">
		SELECT * FROM QUESTIONS
	</select>
	
	<select id="getQuestionOne" parameterType="QuestionsVO" resultType="QuestionsVO">
		SELECT* FROM QUESTIONS
		WHERE QUESTS_NUM=${quests_num}
	</select>



	<insert id="setQuestionInsert" parameterType="QuestionsVO">
		INSERT INTO QUESTIONS
		VALUES (null, #{quests_id}, #{quests_nickname}, #{quests_title}, #{quests_contents}, default, default, 1, 1, 0)
	</insert>
</mapper>